<% content_for :title do %>
  Details of Clinic
<% end %>

<div class="page-header">
  <h1>Details of Clinic</h1>
  <div>
    <%= link_to edit_clinic_path(@clinic), class: 'btn btn-success' do %>
      <%= icon 'fas', 'edit' %>
      Edit
    <% end %>
    <%= link_to clinics_path, class: 'btn btn-outline-secondary' do %>
      <%= icon 'fas', 'bars' %>
      Back
    <% end %>
  </div>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @clinic.name %></dd>

</dl>
